{
  "name": "react-vite-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "dev": "concurrently -k -n client,server -c blue,green \"npm:dev:client\" \"npm:dev:server\"",
    "start:prod": "cross-env API_ENV=prod ts-node server/index.ts",
    "dev:local": "cross-env API_ENV=test concurrently -k -n client,server -c blue,green \"npm:dev:client\" \"npm:dev:server\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:solar-service": "node services/solar-service/index.js",
    "test:matrikkel": "node scripts/testMatrikkel.cjs"
  },
  "dependencies": {
    "@turf/turf": "7.2.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "cors": "2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.2.3",
    "node-cache": "5.1.2",
    "node-fetch": "^3.3.2",
    "proj4": "^2.16.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "soap": "^1.1.11"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "nodemon": "^3.1.10",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  }
}
